{
    "name": "zaabr/yii2-pages",
    "description": "Creating, editing and managing publications on the site.",
    "type": "yii2-extension",
    "keywords": ["yii2","extension"],
    "license": "MIT",
	"minimum-stability": "stable",
    "authors": [
        {
            "name": "ZAABr",
            "email": "zaabr@yandex.ru"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.0"
    },
  "repository": [{
    "type": "vcs",
    "url": "https://github.com/zaabr/yii2-pages"
  	}
  ],
    "extra": [{
        "bootstrap": "zaabr\\yii2-pages\\Bootstrap",
        "branch-alias": {
            "dev-master": "1.0.x"
       		}
    	}
  ],
    "autoload": {
        "psr-4": {
            "zaabr\\pages\\": "src"
        }
    }
}
